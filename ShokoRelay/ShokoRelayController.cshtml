<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://fonts.googleapis.com/css2?family=Sora:wght@100;200;300;400;500;600;700;800&display=swap"
        rel="stylesheet">
  <title>Shoko Relay ❯ Dashboard</title>
  <style>
    body {background:#0f0f0f; color:#ddd; font-family:'Sora', sans-serif; margin:20px}
    button, .plex-login-link {background:#a497b0; border:2px solid #0f0f0f; color:#0f0f0f; font-size:0.9em; font-weight:600; padding:1em; text-align:center; transition:all 0.5s ease}
    button.danger {background:#cc0000}
    button.danger:hover {background:#e74c3c}
    button:hover, .plex-login-link:hover {background:#ea005e}
    h3 {margin:0 0 8px 0}
    hr {background:#000; border:none; height: 1px; margin: 2px 0; padding: 0}
    img, button, section, .plex-login-link {border-radius:0.5rem}
    input, select {height:2.5em}
    input, select, textarea {background:#151515; border:1px solid #0f0f0f; border-radius:0.25em; box-sizing:border-box; color:#ddd; margin:2px 0 6px; outline:none; padding:6px; width:100%}
    input, select, textarea, button {font-family:'Sora', sans-serif}
    input:focus, select:focus, textarea:focus {border:2px solid #94aad1}
    input[type="checkbox"] {accent-color:#94aad1; background:#151515; border:1px solid #0f0f0f; border-radius:1rem; height:18px; margin-right:8px; width:18px}
    label {display:inline-block; margin:6px 0 2px; margin-right:8px}
    section {background:#1c1c1c; margin:12px 2px 0 2px; padding:12px}
    select, textarea {transition:all 0.5s ease}
    select[multiple] {height:auto}
    textarea {height:6em}
    #container {margin:0 auto; max-width:900px}
    .animethemes-svg {display:inline-block; fill:#9f93b8; height:0.8em; width:auto}
    .col-status {border-radius:6px; color:#ddd; display:inline-block; font-weight:600; padding:6px 8px; text-overflow: ellipsis}
    .full {display:flex; flex-wrap:wrap; gap:8px}
    .full > * {flex:1}
    .log {background:#0f0f0f; border-radius:0.25em; margin-top:8px; overflow-x:auto; padding:8px; white-space:pre-wrap}
    .log:empty {display:none}
    .plex::before {color:#ffc826; content:"❯"; margin:0 8px}
  </style>
</head>

<body>
  <div id=container>
    <img
    src="data:image/png;base64,"
    alt="Shoko Relay Dashboard" />
    <div class="full">
      <section>
        <h3 class="plex">Plex Authentication</h3>
        <div class="full" id="plex-auth-action">
          <button id="plex-start">Start Plex Auth</button>
        </div>
        <div class="full">
          <div>
            <label>Libraries</label>
            <select id="plex-libraries" multiple disabled></select>
          </div>
        </div>
      </section>
      <section>
        <h3 class="plex">Plex Automation</h3>
        <div class="full">
          <button id="col-build">Generate Collections</button>
        </div>
        <label id="col-status">Status: Pending...</label>
        <hr />
        <label><input id="plex-scan-vfs" type="checkbox">Scan on VFS Refresh</label>
        <div class="full">
          <div>
            <label>Shoko API Key</label>
            <input id="shoko-api-key" type="password" placeholder="Optional - used for season posters">
          </div>
        </div>
      </section>
    </div>
    <section>
      <h3 class="plex">Virtual File System</h3>
      <div class="full">
        <div>
          <label>Filter [ShokoSeriesIDs] (comma separated)</label>
          <input id="vfs-filter" type="text">
        </div>
      </div>
      <div class="full">
        <label><input id="vfs-run" type="checkbox" checked>Run</label>
        <label><input id="vfs-clean" type="checkbox" checked>Clean VFS</label>
        <label><input id="vfs-dryrun" type="checkbox">Dry Run</label>
      </div>
      <div class="full">
        <button id="vfs-exec">Generate VFS</button>
      </div>
      <div id="vfs-log" class="log"></div>
    </section>
    <section>
      <h3>
        <svg class="animethemes-svg" viewBox="0 0 160 86.6" width="277" height="150">
          <polygon points="56.25 32.48 56.25 75.78 75 86.6 75 0 0 43.3 18.75 54.13 56.25 32.48"></polygon>
          <polygon points="103.75 32.48 141.25 54.13 160 43.3 85 0 85 86.6 103.75 75.78 103.75 32.48"></polygon>
        </svg>
        AnimeThemes: VFS
      </h3>
      <div class="full">
        <div>
          <label>Filter [ShokoSeriesIDs] (comma separated)</label>
          <input id="at-filter-map" type="text">
        </div>
      </div>
      <div class="full">
        <div><label>Map Path</label><input id="at-mapPath" type="text"></div>
        <div><label>Torrent Root</label><input id="at-torrentRoot" type="text"></div>
      </div>
      <div class="full">
        <label><input id="at-dryRun-map" type="checkbox">Dry Run</label>
      </div>
      <div class="full">
        <button id="at-apply">Apply Mapping to VFS</button>
        <button id="at-mapping">Build Mapping [Slow]</button>
      </div>
      <div id="at-map-log" class="log"></div>
    </section>
    <section>
      <h3>
        <svg class="animethemes-svg" viewBox="0 0 160 86.6" width="277" height="150">
          <polygon points="56.25 32.48 56.25 75.78 75 86.6 75 0 0 43.3 18.75 54.13 56.25 32.48"></polygon>
          <polygon points="103.75 32.48 141.25 54.13 160 43.3 85 0 85 86.6 103.75 75.78 103.75 32.48"></polygon>
        </svg>
        AnimeThemes: Theme.mp3
      </h3>
      <div class="full">
        <div><label>Path</label><input id="at-path" type="text"></div>
        <div><label>Slug</label><input id="at-slug" type="text"></div>
        <div><label>Offset</label><input id="at-offset" type="number" value="0"></div>
      </div>
      <div class="full">
        <label><input id="at-force" type="checkbox">Force</label>
        <label><input id="at-dryRun" type="checkbox">Dry Run</label>
        <label><input id="at-batch" type="checkbox">Batch</label>
      </div>
      <div class="full">
        <button id="at-single">Generate</button>
        <button id="at-preview">Preview</button>
      </div>
      <div id="at-log" class="log"></div>
    </section>
    <section>
      <h3 class="plex">Provider Configuration</h3>
      <div id="config-form"></div>
      <div class="full">
        <button id="config-save">Save Config</button>
      </div>
      <div id="config-log" class="log"></div>
    </section>
  </div>
  <script>
      (() => {
        const base = location.pathname.replace(/\/dashboard$/, '');
        const el = (id) => document.getElementById(id);
        const log = (id, msg) => { const node = el(id); if (node) node.textContent = msg; };
        let plexPinId = '';
        let plexPollTimer = null;

        function setColStatus(message, level) {
          const s = el('col-status');
          if (!s) return;
          s.textContent = message || 'pending';
          if (level === 'error') {
            s.style.color = '#cc0000';
          } else if (level === 'ok') {
            s.style.color = '#00ab3f';
          } else if (level === 'running') {
            s.style.color = '#94aad1';
          } else {
            s.style.color = '#dddddd';
          }
        }

        async function ensurePlexEnabled() {
          try {
            const cfgRes = await fetchJson(base + '/config');
            if (!cfgRes.ok || !cfgRes.data) {
              setColStatus('Error loading config', 'error');
              return false;
            }

            const plex = cfgRes.data.PlexLibrary || {};

            if (!plex.ServerUrl || !plex.Token) {
              setColStatus('Plex server or token missing. Link Plex in Plex Auth.', 'error');
              return false;
            }

            const hasSelected = (plex.SelectedLibraries && plex.SelectedLibraries.length > 0) || (plex.LibrarySectionId && plex.LibrarySectionId > 0);
            if (!hasSelected) {
              setColStatus('No Plex library selected. Select a library in Plex Auth.', 'error');
              return false;
            }

            return true;
          } catch (ex) {
            setColStatus('Error checking Plex config', 'error');
            console.error(ex);
            return false;
          }
        }

        async function fetchJson(url, opts) {
          try {
            const res = await fetch(url, opts);
            const text = await res.text();
            try { return { ok: res.ok, data: JSON.parse(text) }; } catch { return { ok: res.ok, data: text }; }
          } catch (err) {
            console.error('fetchJson error for', url, err);
            return { ok: false, data: { error: String(err), message: err?.message ?? String(err), url } };
          }
        }

        function setPlexAction(html) {
          const container = el('plex-auth-action');
          if (container) container.innerHTML = html;
        }

        function setPlexStartAction() {
          setPlexAction('<button id="plex-start">Start Plex Auth</button>');
          const btn = el('plex-start');
          if (btn) btn.onclick = startPlexAuth;
        }

        function setPlexLinkAction(authUrl) {
          setPlexAction('<a id="plex-login" href="' + authUrl + '" target="_blank" class="plex-login-link">Login</a>');
        }

        function setPlexUnlinkAction() {
          setPlexAction('<button id="plex-unlink" class="danger">Unlink Plex</button>');
          const btn = el('plex-unlink');
          if (btn) btn.onclick = unlinkPlex;
        }

        function clearPlexLibraries() {
          const libs = el('plex-libraries');
          if (libs) {
            libs.innerHTML = '';
            libs.disabled = true;
          }
        }

        function populateLibraries(libraries, selectedKeys) {
          const libs = el('plex-libraries');
          if (!libs) return;
          libs.innerHTML = '';
          const selected = new Set((selectedKeys || []).map(String));
          (libraries || []).forEach(lib => {
            if (!lib) return;
            const key = lib.uuid || (lib.serverId ? lib.serverId + '::' + lib.id : String(lib.id));
            const serverName = lib.serverName || lib.ServerName || lib.serverUrl || lib.ServerUrl || '';
            const opt = document.createElement('option');
            opt.value = String(key);
            opt.textContent = (serverName ? serverName + ' ❯ ' : '') + (lib.title || '');
            if (selected.has(String(key))) opt.selected = true;
            libs.appendChild(opt);
          });
          libs.disabled = (libraries || []).length === 0;
          libs.onchange = onLibrariesChange;
        }

        function stopPlexPolling() {
          if (plexPollTimer) {
            clearInterval(plexPollTimer);
            plexPollTimer = null;
          }
        }

        function startPlexPolling() {
          stopPlexPolling();
          plexPollTimer = setInterval(checkPlexAuthStatus, 2000);
        }

        async function startPlexAuth() {
          const btn = el('plex-start');
          if (btn) btn.disabled = true;
          const res = await fetchJson(base + '/plexauth');
          if (!res.ok) {
            console.error('Plex auth start failed', res.data);
            if (btn) btn.disabled = false;
            return;
          }
          plexPinId = res.data.pinId || '';
          if (!plexPinId || !res.data.authUrl) {
            setPlexStartAction();
            return;
          }
          setPlexLinkAction(res.data.authUrl);
          startPlexPolling();
        }

        async function checkPlexAuthStatus() {
          if (!plexPinId) return;
          const res = await fetchJson(base + '/plexauth/status?pinId=' + encodeURIComponent(plexPinId));
          if (res.ok && res.data && res.data.status === 'ok') {
            plexPinId = '';
            stopPlexPolling();
            await refreshPlexState();
          }
        }

        async function refreshPlexState() {
          clearPlexLibraries();

          // Read current config (includes Plex token, discovered libraries)
          const res = await fetchJson(base + '/config');
          if (!res.ok || !res.data) {
            setPlexStartAction();
            return;
          }

          const cfgAll = res.data || {};
          const plex = cfgAll.PlexLibrary || {};

          // Populate Shoko API Key field and wire saving
          const shokoEl = el('shoko-api-key');
          if (shokoEl) {
            shokoEl.value = cfgAll.ShokoApiKey || '';
            shokoEl.onchange = savePlexSettings;
          }
          // Populate plex service toggles

          const scanEl = el('plex-scan-vfs');
          if (scanEl) {
            scanEl.checked = !!plex.ScanOnVfsRefresh;
            scanEl.onchange = savePlexSettings;
          }
          if (!plex.Token) {
            setPlexStartAction();
            return;
          }

          setPlexUnlinkAction();

          const libraries = (plex.DiscoveredLibraries || []).map(l => ({ id: l.Id, title: l.Title, type: l.Type, uuid: l.Uuid, serverId: l.ServerId, serverName: l.ServerName, serverUrl: l.ServerUrl }));
          const selectedKeys = (plex.SelectedLibraries || []).map(s => s.Uuid || (s.ServerId ? s.ServerId + '::' + s.SectionId : String(s.SectionId)));

          if ((!libraries || libraries.length === 0) && (plex.DiscoveredServers && plex.DiscoveredServers.length > 0)) {
            const rr = await fetchJson(base + '/plex/libraries/refresh', { method: 'POST' });
            if (rr.ok && rr.data && rr.data.libraries) {
              const libs = (rr.data.libraries || []).map(l => ({ id: l.Id, title: l.Title, type: l.Type, uuid: l.Uuid, serverId: l.ServerId, serverName: l.ServerName, serverUrl: l.ServerUrl }));
              populateLibraries(libs, selectedKeys);
              return;
            } else {
              // fall through to show empty state
            }
          }

          populateLibraries(libraries, selectedKeys);

          // No Plex libraries found.
        }

        async function onLibrariesChange() {
          const libs = el('plex-libraries');
          if (!libs) return;
          const selected = Array.from(libs.selectedOptions).map(o => o.value);

          // Load current config, update selected libraries, and save
          const cfgRes = await fetchJson(base + '/config');
          if (!cfgRes.ok || !cfgRes.data) {
            return;
          }

          const cfg = cfgRes.data;
          const discovered = cfg.PlexLibrary?.DiscoveredLibraries || [];

          const newSelected = (discovered || []).filter(d => {
            const key = d.Uuid || (d.ServerId ? d.ServerId + '::' + d.Id : String(d.Id));
            return selected.includes(key);
          }).map(d => ({ SectionId: d.Id, Title: d.Title, Type: d.Type, Uuid: d.Uuid, ServerId: d.ServerId, ServerName: d.ServerName, ServerUrl: d.ServerUrl }));

          cfg.PlexLibrary.SelectedLibraries = newSelected;
          cfg.PlexLibrary.LibrarySectionId = newSelected[0]?.SectionId || 0;
          cfg.PlexLibrary.SelectedLibraryName = newSelected[0]?.Title || '';
          cfg.PlexLibrary.SectionUuid = newSelected[0]?.Uuid || '';
          cfg.PlexLibrary.ServerIdentifier = newSelected[0]?.ServerId || '';
          cfg.PlexLibrary.ServerUrl = newSelected[0]?.ServerUrl || cfg.PlexLibrary.ServerUrl || '';

          const saveRes = await fetchJson(base + '/config', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify(cfg) });
          if (!saveRes.ok) {
            return;
          }
        }

        async function savePlexSettings() {
          try {
            const cfgRes = await fetchJson(base + '/config');
            if (!cfgRes.ok || !cfgRes.data) {
              setColStatus('Error loading config', 'error');
              return false;
            }
            const cfg = cfgRes.data;
            cfg.PlexLibrary = cfg.PlexLibrary || {};
            cfg.PlexLibrary.ScanOnVfsRefresh = !!el('plex-scan-vfs')?.checked;
            cfg.ShokoApiKey = el('shoko-api-key')?.value || '';
            const saveRes = await fetchJson(base + '/config', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify(cfg) });
            if (!saveRes.ok) {
              setColStatus('Error saving Plex settings', 'error');
              console.error(saveRes.data);
              return false;
            }
            setColStatus('Plex settings saved', 'ok');
            return true;
          } catch (ex) {
            setColStatus('Error saving Plex settings', 'error');
            console.error(ex);
            return false;
          }
        }

        async function unlinkPlex() {
          const res = await fetchJson(base + '/plex/unlink', { method: 'POST' });
          if (!res.ok) {
            console.error('Failed to revoke Plex token.');
            return;
          }
          clearPlexLibraries();
          setPlexStartAction();
        }

        el('vfs-exec').onclick = async () => {
          const params = new URLSearchParams();
          const filter = el('vfs-filter').value;
          if (filter) params.set('filter', filter);
          params.set('clean', el('vfs-clean').checked ? 'true' : 'false');
          params.set('dryRun', el('vfs-dryrun').checked ? 'true' : 'false');
          params.set('run', el('vfs-run').checked ? 'true' : 'false');
          const res = await fetchJson(base + '/vfs?' + params.toString());
          log('vfs-log', JSON.stringify(res.data, null, 2));
        };

        el('col-build').onclick = async () => {
          setColStatus('Checking Plex configuration...', 'running');
          const ok = await ensurePlexEnabled();
          if (!ok) return;

          setColStatus('Running collection build...', 'running');
          const params = new URLSearchParams();

          const res = await fetchJson(base + '/plex/collections/build?' + params.toString());
          if (!res.ok) {
            setColStatus('Error: ' + (res.data?.message || 'Request failed'), 'error');
            console.error(res.data);
            return;
          }

          const data = res.data;
          if (data?.status === 'ok') {
            const uploadedText = data?.uploaded !== undefined ? `, uploaded ${data.uploaded}` : '';
            setColStatus(`OK: processed ${data.processed}, created ${data.created}${uploadedText}, skipped ${data.skipped}, errors ${data.errors}`, 'ok');
          } else {
            setColStatus('Error: ' + (data?.message || JSON.stringify(data)), 'error');
          }
        };

        el('at-single').onclick = async () => {
          const params = new URLSearchParams();
          const path = el('at-path').value;
          const slug = el('at-slug').value;
          const offset = el('at-offset').value;
          const filter = el('at-filter').value;
          if (path) params.set('path', path);
          if (slug) params.set('slug', slug);
          if (offset) params.set('offset', offset);
          if (filter) params.set('filter', filter);
          if (el('at-force').checked) params.set('force', 'true');
          if (el('at-dryRun').checked) params.set('dryRun', 'true');
          const res = await fetchJson(base + '/animethemes?' + params.toString());
          log('at-log', JSON.stringify(res.data, null, 2));
        };
        el('at-batch').onclick = async () => {
          const params = new URLSearchParams();
          const path = el('at-path').value;
          const slug = el('at-slug').value;
          const offset = el('at-offset').value;
          const filter = el('at-filter').value;
          if (path) params.set('path', path);
          if (slug) params.set('slug', slug);
          if (offset) params.set('offset', offset);
          if (filter) params.set('filter', filter);
          if (el('at-force').checked) params.set('force', 'true');
          if (el('at-dryRun').checked) params.set('dryRun', 'true');
          if (el('at-batch').checked) params.set('batch', 'true');
          const res = await fetchJson(base + '/animethemes?' + params.toString());
          log('at-log', JSON.stringify(res.data, null, 2));
        };
        el('at-preview').onclick = () => {
          const params = new URLSearchParams();
          const path = el('at-path').value;
          const slug = el('at-slug').value;
          const offset = el('at-offset').value;
          const filter = el('at-filter').value;
          if (path) params.set('path', path);
          if (slug) params.set('slug', slug);
          if (offset) params.set('offset', offset);
          if (filter) params.set('filter', filter);
          if (el('at-force').checked) params.set('force', 'true');
          if (el('at-dryRun').checked) params.set('dryRun', 'true');
          params.set('play', 'true');
          window.open(base + '/animethemes?' + params.toString(), '_blank');
        };
        el('at-mapping').onclick = async () => {
          const params = new URLSearchParams();
          const mapPath = el('at-mapPath').value;
          const torrentRoot = el('at-torrentRoot').value;
          const filter = el('at-filter-map').value;
          if (mapPath) params.set('mapPath', mapPath);
          if (torrentRoot) params.set('torrentRoot', torrentRoot);
          if (filter) params.set('filter', filter);
          if (el('at-dryRun-map').checked) params.set('dryRun', 'true');
          params.set('mapping', 'true');
          const res = await fetchJson(base + '/animethemes?' + params.toString());
          log('at-map-log', JSON.stringify(res.data, null, 2));
        };
        el('at-apply').onclick = async () => {
          const params = new URLSearchParams();
          const mapPath = el('at-mapPath').value;
          const torrentRoot = el('at-torrentRoot').value;
          const filter = el('at-filter-map').value;
          if (mapPath) params.set('mapPath', mapPath);
          if (torrentRoot) params.set('torrentRoot', torrentRoot);
          if (filter) params.set('filter', filter);
          if (el('at-dryRun-map').checked) params.set('dryRun', 'true');
          params.set('applyMapping', 'true');
          const res = await fetchJson(base + '/animethemes?' + params.toString());
          log('at-map-log', JSON.stringify(res.data, null, 2));
        };

        function getValueByPath(obj, path) {
          return path.split('.').reduce((o, k) => (o ? o[k] : undefined), obj);
        }
        function setValueByPath(obj, path, value) {
          const parts = path.split('.');
          let cur = obj;
          for (let i = 0; i < parts.length - 1; i++) {
            const key = parts[i];
            if (!cur[key]) cur[key] = {};
            cur = cur[key];
          }
          cur[parts[parts.length - 1]] = value;
        }

        async function loadConfig() {
          const schemaRes = await fetchJson(base + '/config/schema');
          const configRes = await fetchJson(base + '/config');
          if (!schemaRes.ok || !configRes.ok) { log('config-log', 'Failed to load config.'); return; }
          const schema = schemaRes.data.properties || [];
          const config = configRes.data;
          const container = el('config-form');
          container.innerHTML = '';
          schema.forEach(p => {
            const wrap = document.createElement('div');
            const label = document.createElement('label');
            label.textContent = (p.Display || p.Path) + (p.Description ? ' ❯ ' + p.Description : '');
            let input;
            const value = getValueByPath(config, p.Path);
            if (p.Type === 'bool') {
              input = document.createElement('input');
              input.type = 'checkbox';
              input.checked = !!value;
              wrap.appendChild(input);
              wrap.appendChild(label);
            } else {
              wrap.appendChild(label);
              if (p.Type === 'enum') {
                input = document.createElement('select');
                (p.EnumValues || []).forEach(ev => {
                  const opt = document.createElement('option');
                  opt.value = ev.value;
                  opt.textContent = ev.name;
                  if (String(ev.value) === String(value)) opt.selected = true;
                  input.appendChild(opt);
                });
              } else if (p.Type === 'number') {
                input = document.createElement('input');
                input.type = 'number';
                input.value = value !== undefined && value !== null ? value : '';
              } else if (p.Type === 'json') {
                input = document.createElement('textarea');
                input.value = value ? JSON.stringify(value, null, 2) : '';
              } else if (p.Path === 'CrossoverOverrides' || p.Path === 'TagBlacklist') {
                input = document.createElement('textarea');
                input.value = value !== undefined && value !== null ? value : '';
              } else {
                input = document.createElement('input');
                input.type = 'text';
                input.value = value !== undefined && value !== null ? value : '';
              }
              wrap.appendChild(input);
            }
            input.dataset.path = p.Path;
            input.dataset.type = p.Type;
            container.appendChild(wrap);
          });

          el('config-save').onclick = async () => {
            const updated = JSON.parse(JSON.stringify(config));
            const inputs = container.querySelectorAll('[data-path]');
            for (const input of inputs) {
              const path = input.dataset.path;
              const type = input.dataset.type;
              let val;
              if (type === 'bool') {
                val = input.checked;
              } else if (type === 'enum' || type === 'number') {
                val = input.value === '' ? 0 : Number(input.value);
              } else if (type === 'json') {
                val = input.value ? JSON.parse(input.value) : null;
              } else {
                val = input.value;
              }
              setValueByPath(updated, path, val);
            }
            const res = await fetchJson(base + '/config', { method: 'POST', headers: { 'Content-Type': 'application/json' }, body: JSON.stringify(updated) });
            log('config-log', JSON.stringify(res.data, null, 2));
          };
        }

        setPlexStartAction();
        refreshPlexState();
        loadConfig();
      })();
  </script>
</body>

